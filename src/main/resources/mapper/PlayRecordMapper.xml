<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javacode.springboot_program.mapper.PlayRecordMapper">
    <insert id="saveRecord" parameterType="PlayRecord">
    INSERT INTO `xdclass`.`play_record` (`user_id`, `video_id`, `current_num`, `episode_id`, `create_time`)
    VALUES (#{userId,jdbcType=INTEGER},#{videoId,jdbcType=INTEGER},#{currentNum,jdbcType=INTEGER},#{episodeId,jdbcType=INTEGER},#{createTime,jdbcType=TIMESTAMP})
    </insert>
</mapper>

